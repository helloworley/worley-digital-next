eatedAt":"2020-06-09T09:05:19.965Z","updatedAt":"2020-06-09T09:05:19.965Z","environment":{"sys":{"id":"master","type":"Link","linkType":"Environment"}},"revision":1,"contentType":{"sys":{"type":"Link","linkType":"ContentType","id":"backendTechnology"}}},"fields":{"name":{"en":"Wordpress CMS"},"logo":{"en":{"sys":{"space":{"sys":{"type":"Link","linkType":"Space","id":"p0ly3e00wdqi"}},"id":"x3lQAcMqj5ftaEm7EtbUv","type":"Asset","createdAt":"2020-06-09T09:05:05.396Z","updatedAt":"2020-06-09T09:05:05.396Z","environment":{"sys":{"id":"master","type":"Link","linkType":"Environment"}},"revision":1},"fields":{"title":{"en":"wordpress"},"file":{"en":{"url":"https://images.ctfassets.net/p0ly3e00wdqi/x3lQAcMqj5ftaEm7EtbUv/0fe9602fb6c6317e57b1d5356959a0bd/wordpress.svg","details":{"size":42253,"image":{"width":100,"height":100}},"fileName":"wordpress.svg","contentType":"image/svg+xml"}}}}},"link":{"en":"https://wordpress.org/"}}}},"image":{"en":{"sys":{"space":{"sys":{"type":"Link","linkType":"Space","id":"p0ly3e00wdqi"}},"id":"7ykBx5cLXcU2EedTfZrKBp","type":"Asset","createdAt":"2020-06-09T22:55:17.517Z","updatedAt":"2020-06-09T22:55:17.517Z","environment":{"sys":{"id":"master","type":"Link","linkType":"Environment"}},"revision":1},"fields":{"title":{"en":"PKMASA Desktop Mobile"},"file":{"en":{"url":"https://images.ctfassets.net/p0ly3e00wdqi/7ykBx5cLXcU2EedTfZrKBp/ff197197efb5c456ab771408d6f93601/pkmasa-desktop-mobile.jpg","details":{"size":247496,"image":{"width":2000,"height":1500}},"fileName":"pkmasa-desktop-mobile.jpg","contentType":"image/jpeg"}}}}},"content":{"en":{"data":{},"content":[{"data":{},"content":[{"data":{},"marks":[],"value":"PK MASA is Masa Suzuki’s online portfolio and education al services website. He is a ParkourEDU certified parkour coach, acrobatic dancer, Movement coach, Fighting Monkey teacher and rehabilitation specialist - the ultimate physical fitness guru.\n\nMasa's website now markets his physical educational services in both Japanese and English, so we helped him create an easily updatable custom Wordpress website that he could add content to and expand in the future.\n\nWe chose Wordpress because:","nodeType":"text"}],"nodeType":"paragraph"},{"data":{},"content":[{"data":{},"content":[{"data":{},"content":[{"data":{},"marks":[],"value":"Trustworthiness. Around 37% of the web is built on Wordpress (according to Wordpress).","nodeType":"text"}],"nodeType":"paragraph"}],"nodeType":"list-item"},{"data":{},"content":[{"data":{},"content":[{"data":{},"marks":[],"value":"Maintainability. It’s easy to update your website’s content by yourself. Masa was looking for a powerful solution that he could customize to his liking.","nodeType":"text"}],"nodeType":"paragraph"}],"nodeType":"list-item"},{"data":{},"content":[{"data":{},"content":[{"data":{},"marks":[],"value":"SEO. Because Wordpress offers plugins like Yoast to optimize SEO, Masa was able to easily rise to the top in Google Search results.","nodeType":"text"}],"nodeType":"paragraph"}],"nodeType":"list-item"},{"data":{},"content":[{"data":{},"content":[{"data":{},"marks":[],"value":"Community. Wordpress has been around since 2003, and during that time its developers have worked hard to create a safe platform that anyone can build upon. Since then, developers have built an infinite number of plugins that others can easily use to increase the site’s effectiveness.","nodeType":"text"}],"nodeType":"paragraph"}],"nodeType":"list-item"}],"nodeType":"unordered-list"},{"data":{},"content":[{"data":{},"marks":[],"value":"","nodeType":"text"}],"nodeType":"paragraph"}],"nodeType":"document"},"ja":{"data":{},"content":[{"data":{},"content":[{"data":{},"marks":[],"value":"PK MASAは、鈴木雅のオンラインポートフォリオと教育サービスのウェブサイトです。彼は、パルクールEDU認定パルクールコーチ、アクロバティックダンサー、ムーブメントコーチ、ファイティングモンキーティーチャー、リハビリテーションスペシャリストであり、究極の体力の第一人者です。","nodeType":"text"}],"nodeType":"paragraph"},{"data":{},"content":[{"data":{},"marks":[],"value":"マサのウェブサイトは現在、彼の体育サービスを日本語と英語の両方で販売しているので、将来的にコンテンツを追加したり拡張したりできる、簡単に更新できるカスタムWordpressウェブサイトの作成を支援しました。","nodeType":"text"}],"nodeType":"paragraph"},{"data":{},"content":[{"data":{},"marks":[],"value":"Wordpressを選択した理由は次のとおりです。","nodeType":"text"}],"nodeType":"paragraph"},{"data":{},"content":[{"data":{},"content":[{"data":{},"content":[{"data":{},"marks":[],"value":"信頼性。 Webの約37％がWordpressで構築されています（Wordpressによると）。","nodeType":"text"}],"nodeType":"paragraph"}],"nodeType":"list-item"},{"data":{},"content":[{"data":{},"content":[{"data":{},"marks":[],"value":"保守性。ウェブサイトのコンテンツを自分で更新するのは簡単です。マサは自分の好みに合わせてカスタマイズできる強力なソリューションを探していました。\nSEO。 Wordpressは、SEOを最適化するためにYoastのようなプラグインを提供しているため、MasaはGoogle検索結果のトップに簡単に上がることができました。","nodeType":"text"}],"nodeType":"paragraph"}],"nodeType":"list-item"},{"data":{},"content":[{"data":{},"content":[{"data":{},"marks":[],"value":"コミュニティ。 Wordpressは2003年以来存在しており、その期間中、開発者たちは誰もが構築できる安全なプラットフォームを作成するために懸命に取り組んできました。それ以来、開発者は無数のプラグインを作成しており、他の人が簡単に使用してサイトの効果を高めることができます。","nodeType":"text"}],"nodeType":"paragraph"}],"nodeType":"list-item"}],"nodeType":"unordered-list"},{"data":{},"content":[{"data":{},"marks":[],"value":"","nodeType":"text"}],"nodeType":"paragraph"}],"nodeType":"document"}}}